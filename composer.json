{
    "name": "vadimcontenthunter/admin-panel",
    "description": "my database",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Vadim",
            "email": "project.k.vadim@gmail.com"
        }
    ],
    "require": {
        "psr/log":"^3.0",
        "vadimcontenthunter/my-db": "^1.2",
        "vadimcontenthunter/json-rpc": "^1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "vadimcontenthunter\\AdminPanel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "vadimcontenthunter\\AdminPanel\\tests\\": "tests/"
        }
    },
    "scripts": {
        "phpcs": [
            "./vendor/bin/phpcs"
        ],
        "phpunit": [
            "./vendor/bin/phpunit"
        ],
        "phpstan": [
            "./vendor/bin/phpstan analyse"
        ]
    }
}
